<div id="clearance" class="sign-up">
  <h2><%= t(".title") %></h2>
  <%= form_with model: @user, url: users_path do |f| %>
    <%= f.label :username, "username" %>
    <%= f.text_field :username %>
    <%#  %>
    <%= f.label :email, "email" %>
    <%= f.email_field :email %>
    <%#  %>
    <%= f.label :password, "password" %>
    <%= f.password_field :password %>
    <%#  %>
    <%= f.submit "Sign up" %>
  <% end %>
</div>
